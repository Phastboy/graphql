{
  "name": "@boundless/boundless-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@boundless/boundless"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/boundless-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@boundless/boundless:build",
          "@boundless/boundless:serve"
        ]
      }
    }
  }
}
